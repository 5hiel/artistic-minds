{
  "name": "gifted-minds",
  "main": "expo-router/entry",
  "version": "1.0.7",
  "scripts": {
    "dev": "expo start",
    "dev:android": "expo run:android",
    "dev:ios": "expo run:ios",
    "dev:web": "expo start --web",
    "start": "expo start",
    "web": "expo start --web",
    "quality:lint": "expo lint",
    "quality:typecheck": "npx tsc --noEmit",
    "quality:full": "npm run quality:lint && npm run quality:typecheck && npm run test:ci",
    "lint": "expo lint",
    "typecheck": "npx tsc --noEmit",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "build:preview": "eas build --platform all --profile preview",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "submit:all": "npm run submit:ios && npm run submit:android",
    "release:patch": "npm version patch && git push origin main --tags",
    "release:minor": "npm version minor && git push origin main --tags",
    "release:major": "npm version major && git push origin main --tags",
    "delivery:check-status": "node ./scripts/delivery/check-app-store-status.js",
    "delivery:build": "eas build --platform ios --profile production",
    "delivery:release": "node ./scripts/delivery/generate-release-notes.js",
    "quality:coverage": "node ./scripts/quality/check-coverage.js",
    "quality:bugs": "node ./scripts/quality/analyze-bugs.js",
    "monitor:feedback": "node ./scripts/monitoring/sync-app-store-feedback.js",
    "utils:version": "node ./scripts/utilities/version-manager.js",
    "utils:reset": "node ./scripts/utilities/project-reset.js",
    "utils:logs": "./scripts/utilities/manage-logs.sh",
    "test": "node --max-old-space-size=6144 node_modules/.bin/jest --config=config/jest.config.js --runInBand",
    "test:watch": "node --max-old-space-size=6144 node_modules/.bin/jest --config=config/jest.config.js --watch --runInBand",
    "test:coverage": "node --max-old-space-size=6144 node_modules/.bin/jest --config=config/jest.config.js --coverage --runInBand",
    "test:ci": "node --max-old-space-size=4096 node_modules/.bin/jest --config=config/jest.ci.config.js --ci --watchAll=false",
    "test:lib": "node --max-old-space-size=2048 --expose-gc node_modules/.bin/jest --config=config/jest.config.lib.js",
    "test:lib:watch": "npm run test:lib -- --watch",
    "test:lib:coverage": "npm run test:lib -- --coverage",
    "test:adaptive": "node --max-old-space-size=4096 node_modules/.bin/jest --config=config/jest.config.js __tests__/adaptiveEngine --runInBand --testTimeout=180000",
    "test:hooks": "node --max-old-space-size=1024 node_modules/.bin/jest --config=config/jest.config.js __tests__/hooks --maxWorkers=2",
    "test:personas": "node --max-old-space-size=4096 node_modules/.bin/jest --config=config/jest.config.js __tests__/personas --runInBand --testTimeout=300000",
    "test:personas:enhanced": "node --max-old-space-size=4096 node_modules/.bin/jest --config=config/jest.config.personas.js __tests__/personas/enhanced-configurable-persona.test.ts --runInBand",
    "test:personas:atomic": "node --max-old-space-size=4096 node_modules/.bin/jest --config=config/jest.config.personas.js __tests__/personas/atomic-chunk-processor.test.ts --runInBand",
    "test:personas:chunk": "node --max-old-space-size=4096 node_modules/.bin/jest --config=config/jest.config.js __tests__/personas --runInBand --testTimeout=180000",
    "test:persona:ma": "PERSONA=ma npm run test:personas:chunk -- --testNamePattern=\"Single persona chunk\"",
    "test:persona:omi": "PERSONA=omi npm run test:personas:chunk -- --testNamePattern=\"Single persona chunk\"",
    "test:persona:mumu": "PERSONA=mumu npm run test:personas:chunk -- --testNamePattern=\"Single persona chunk\"",
    "test:persona:ira": "PERSONA=ira npm run test:personas:chunk -- --testNamePattern=\"Single persona chunk\"",
    "personas:run-batch": "node scripts/run-persona-batch.js",
    "personas:run-single": "node scripts/run-persona-batch.js --persona",
    "personas:aggregate": "node scripts/aggregate-persona-results.js",
    "personas:monitor": "node scripts/personas/monitor-progress.js",
    "personas:orchestrate": "node scripts/personas/cross-persona-orchestrator.js",
    "test:journeys": "npm run test:personas",
    "test:quick": "npm run test:lib && npm run test:hooks",
    "quality-check": "npm run lint && npm run typecheck && npm run test:ci && npx expo-doctor",
    "quality-check-full": "npm run lint && npm run typecheck && npm run test:ci && npm run test:journeys && npx expo-doctor",
    "pre-release": "npm run quality-check-full",
    "monitoring:clear": "node -e \"require('./lib/monitoring').monitoring.clearLocalData()\"",
    "monitoring:export": "node -e \"require('./lib/monitoring').monitoring.getLocalData().then(data => console.log(JSON.stringify(data, null, 2)))\"",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@octokit/rest": "^22.0.0",
    "@react-native-async-storage/async-storage": "~2.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "better-sqlite3": "^12.2.0",
    "expo": "~53.0.23",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.7",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-store-review": "~8.1.5",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "jsonwebtoken": "^9.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@jest/globals": "^29.5.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.5.0",
    "jest-expo": "~53.0.10",
    "playwright": "^1.55.0",
    "react-test-renderer": "^19.1.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
